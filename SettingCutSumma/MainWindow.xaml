<Window x:Class="SettingCutSumma.MainWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SettingCutSumma"
             mc:Ignorable="d" 
             MinWidth="285" Loaded="UserControl_Loaded" Width="250" Height="400" WindowStyle="ToolWindow" ResizeMode="NoResize" Title="Настройки файла резки">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Colors.xaml"/>
                <ResourceDictionary Source="Styles/Styles.xaml"/>
                <ResourceDictionary Source="Resources/Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid Margin="0,0,0,0">
        <StackPanel>
           <ComboBox Name="Velosity" Width="60" Margin="118,15,0,0" HorizontalAlignment="Left"/>
           <ComboBox Name="Overcut" Width="60" Margin="118,6,0,0" HorizontalAlignment="Left"/>
           <TextBlock Text="Скорость резки" Margin="15,-50,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
           <TextBlock Text="Перерез кривой" Margin="15,-22,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
           <TextBlock Text="Размытие кривой" Margin="15,7,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
           <CheckBox Name="Smothing" Margin="118,-15,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>

        </StackPanel>
    </Grid>


</Window>
